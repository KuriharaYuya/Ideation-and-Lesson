<h1>Lifelogs#new</h1>
<%= form_with(model:@lifelog,local: true ) do |f| %>
<%=f.label :title %>
<br>
<%=f.text_field :title  %>
<br>
<%=f.label :calender  %>
<br>
<%=f.file_field :calender  %>
<br>
<%=f.label :screen_time  %>
<br>
<%=f.file_field :screen_time  %>
<br>
<%=f.label :assumption_minutes  %>
<br>
<%=f.number_field :assumption_minutes  %>
<br>
<br>
<%=f.label :log_date, "date" %>
<br>
<%=f.date_field :log_date  %>
<br>
<br>
<%=f.submit "create", disable_with: 'proccessing...'  %>
<% end  %>